<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <IncludeBuildOutput>false</IncludeBuildOutput>
    <SuppressDependenciesWhenPacking>true</SuppressDependenciesWhenPacking>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>Microsoft.VisualStudio.Extensibility.Testing.SupportFiles</PackageId>
    <PackageVersion>1.1.0</PackageVersion>
    <Authors>Microsoft</Authors>
    <PackageProjectUrl>https://github.com/microsoft/vs-extension-testing</PackageProjectUrl>
    <RepositoryUrl>https://github.com/microsoft/vs-extension-testing</RepositoryUrl>
    <Description>This package contains assemblies that vs-extension-testing depends on but does not build itself.</Description>
    <Copyright>Â© Microsoft Corporation. All rights reserved.</Copyright>
    <PackageTags>VSSDK</PackageTags>
    <DevelopmentDependency>true</DevelopmentDependency>
  </PropertyGroup>

  <ItemGroup>
    <!-- Package information -->
    <Content Include="EULA.rtf" PackagePath="" />

    <!-- Visual Studio 2012 -->
    <Content Include="11\Microsoft.VisualStudio.ExtensionManager.dll" PackagePath="build/ref/11" />
    <Content Include="11\Microsoft.VisualStudio.ExtensionManager.Implementation.dll" PackagePath="build/ref/11" />

    <!-- Visual Studio 2013 -->
    <Content Include="12\Microsoft.VisualStudio.ExtensionManager.dll" PackagePath="build/ref/12" />
    <Content Include="12\Microsoft.VisualStudio.ExtensionManager.Implementation.dll" PackagePath="build/ref/12" />

    <!-- Visual Studio 2015 -->
    <Content Include="14\Microsoft.VisualStudio.ExtensionManager.dll" PackagePath="build/ref/14" />
    <Content Include="14\Microsoft.VisualStudio.ExtensionManager.Implementation.dll" PackagePath="build/ref/14" />

    <!-- Visual Studio 2017 -->
    <Content Include="15\Microsoft.VisualStudio.ExtensionEngine.dll" PackagePath="build/ref/15" />
    <Content Include="15\Microsoft.VisualStudio.ExtensionManager.dll" PackagePath="build/ref/15" />
    <Content Include="15\Microsoft.VisualStudio.ExtensionManager.Implementation.dll" PackagePath="build/ref/15" />

    <!-- Visual Studio 2019 -->
    <Content Include="16\Microsoft.VisualStudio.ExtensionEngine.dll" PackagePath="build/ref/16" />
    <Content Include="16\Microsoft.VisualStudio.ExtensionManager.dll" PackagePath="build/ref/16" />
    <Content Include="16\Microsoft.VisualStudio.ExtensionManager.Implementation.dll" PackagePath="build/ref/16" />

    <!-- Visual Studio 2022 -->
    <Content Include="17\Microsoft.VisualStudio.ExtensionEngine.dll" PackagePath="build/ref/17" />
    <Content Include="17\Microsoft.VisualStudio.ExtensionManager.dll" PackagePath="build/ref/17" />
    <Content Include="17\Microsoft.VisualStudio.ExtensionManager.Implementation.dll" PackagePath="build/ref/17" />
  </ItemGroup>

</Project>
